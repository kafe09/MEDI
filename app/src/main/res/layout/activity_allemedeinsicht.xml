<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--tools:context="com.example.katharinafeiertag.mediary.alleMedEinsichtActivity">-->

    <TextView
        android:id="@+id/tf_editText"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="14dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/Anzeige_Hausapotheke"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="65dp" />

    <ListView
        android:id="@+id/lv_medikamente"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tf_editText"
        android:layout_marginTop="62dp" />

    <EditText
        android:id="@+id/tf_suche"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tf_editText"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/welches_medikament_suchen_sie"
        android:textAlignment="textStart"
        android:textColor="@android:color/darker_gray"
        android:textSize="14sp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tf_editText"
        android:text="Button" />


</RelativeLayout>
