<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.katharinafeiertag.mediary.HausapothekeActivity">

    <TextView
        android:id="@+id/tf_vorhanden"
        android:layout_width="162dp"
        android:layout_height="28dp"
        android:layout_alignEnd="@+id/tf_editText"
        android:layout_alignParentTop="true"
        android:layout_marginTop="38dp"
        android:text="@string/Anzeige_Hausapotheke"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/tf_editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tf_vorhanden"
        android:layout_marginTop="94dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="65dp" />
</RelativeLayout>
