<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.katharinafeiertag.mediary.MainActivity">

    <ImageButton
        android:id="@+id/bt_suitcase"
        android:layout_width="152dp"
        android:layout_height="160dp"
        android:background="@android:color/background_light"
        android:contentDescription="Hausapotheke"
        app:layout_constraintBaseline_creator="@android:integer/config_shortAnimTime"
        app:srcCompat="@drawable/apothekebt"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="11dp" />

    <ImageButton
        android:id="@+id/bt_setting"
        android:layout_width="150dp"
        android:layout_height="136dp"
        android:background="@android:color/transparent"
        android:contentDescription="Einstellungen"
        android:onClick="onSettingsClick"
        app:layout_constraintBaseline_creator="@android:integer/config_shortAnimTime"
        app:srcCompat="@drawable/settingsbt"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="348dp" />

    <ImageButton
        android:id="@+id/bt_med"
        android:layout_width="137dp"
        android:layout_height="131dp"
        android:background="@android:color/background_light"
        android:contentDescription="regelmäßig einzunehmende Mediakmente"
        app:layout_constraintBaseline_creator="@android:integer/config_shortAnimTime"
        app:srcCompat="@drawable/medbt"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/bt_user"
        android:layout_width="127dp"
        android:layout_height="149dp"
        android:background="@android:color/background_light"
        android:contentDescription="Benutzerprofil"
        android:onClick="onUserClick"
        app:layout_constraintBaseline_creator="@android:integer/config_shortAnimTime"
        app:srcCompat="@drawable/userbt"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="171dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:contentDescription="Apotheken in Ihrer Nähe"
        app:srcCompat="@drawable/apotheken"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="342dp" />

</android.support.constraint.ConstraintLayout>
